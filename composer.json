{
    "name": "lav45/yii2-ajax-create",
    "description": "This widget is designed to quickly create missing related entities when filling out the form.",
    "keywords": [
        "yii2",
        "extension",
        "widget",
        "ajax"
    ],
    "homepage": "https://github.com/lav45/yii2-ajax-create",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Aleksey Loban",
            "email": "lav451@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "2.0.*",
        "bower-asset/jquery-form": "4.*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "suggest": {
        "yiisoft/yii2-bootstrap": "It is necessary to display the content in a modal window.",
        "yiisoft/yii2-bootstrap4": "It is necessary to display the content in a modal window.",
        "yiisoft/yii2-bootstrap5": "It is necessary to display the content in a modal window."
    },
    "autoload": {
        "psr-4": {
            "lav45\\widget\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
